# 'source' is where bundler will go by default
source 'https://rubygems.org'

# This is where we tell it to get the version for GitHub
# instead of using the version from https://rubygems.org
gem 'sunlight-congress', git: 'git@github.com:steveklabnik/sunlight-congress.git'
gem 'pry'
gem 'rake'
gem 'minitest'
gem 'terminal-table'
